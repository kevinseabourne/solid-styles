{
  "name": "@solid-styles/lightning",
  "version": "0.1.0",
  "description": "Lightning CSS zero-runtime optimization for Solid Styles",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts",
      "require": "./index.ts"
    },
    "./vinxi": {
      "types": "./transforms/vinxi-plugin.ts",
      "import": "./transforms/vinxi-plugin.ts",
      "require": "./transforms/vinxi-plugin.ts"
    },
    "./vite": {
      "types": "./transforms/vite-plugin.ts",
      "import": "./transforms/vite-plugin.ts",
      "require": "./transforms/vite-plugin.ts"
    },
    "./runtime": {
      "types": "./runtime/enhanced-styled.ts",
      "import": "./runtime/enhanced-styled.ts",
      "require": "./runtime/enhanced-styled.ts"
    }
  },
  "dependencies": {
    "lightningcss": "^1.22.0",
    "solid-js": "^1.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vinxi": "^0.3.0"
  },
  "peerDependencies": {
    "solid-js": "^1.8.0",
    "vite": "^4.0.0 || ^5.0.0",
    "vinxi": "^0.3.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "vinxi": {
      "optional": true
    }
  },
  "keywords": [
    "solid-styles",
    "solid-js",
    "lightning-css",
    "zero-runtime",
    "css-in-js",
    "vinxi",
    "vite"
  ],
  "license": "MIT"
}
